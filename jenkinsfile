pipeline{
    agent any
    tools{
        maven "mvn"
    }
    stages{
        stage('checkout'){
            steps{
                sh"""
                git clone 'https://github.com/siraparapudevops/spring-boot-hello-world.git'
                """
            }
        }
        stage('maven build'){
            steps{
                sh"""
                mvn install
                """
            }
        }
    }
}